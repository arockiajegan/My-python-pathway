from enum import Enum, unique, auto
class FRUIT(Enum):
    APPLE =1 
    BANANA =2 
    GUAVA =3
    POMOGRANETE =4
    ORANGE =5
    PEAR = auto()
print(FRUIT.APPLE)
print(type(FRUIT.BANANA))
print(repr(FRUIT.BANANA))
print(FRUIT.PEAR.value)


my_fruits  = {}
my_fruits[FRUIT.BANANA] = "Welcome to my Code Snippet"
print(my_fruits[FRUIT.BANANA])
# @unique will finds the duplicate value of the enumeration 
