#map function in python 
def cel_to_far(temp):
    return (temp * 9/5) + 32

def far_to_cel(temp):
    return (temp - 32) *  5/9

cel_temps = [0,12,36,99]
far_temps = [111, 96, 88, 102.5]

print("far_values are:", list(map(cel_to_far, cel_temps)))
print("celius values are:", list(map(far_to_cel, far_temps)))
